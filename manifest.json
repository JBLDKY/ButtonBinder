{
  "manifest_version": 3,
  "name": "ButtonBinder",
  "version": "1.2",
  "description": "Bind buttons to keyboard shortcuts!",
  "options_page": "options.html",
  "content_scripts": [
    {
      "js": [ "jquery.min.js", "submitter.js", "options.js"],
      "type": "module",
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
  "storage"
  ]
}
